    <!-- Control Templates & Styles -->
    <!-- TextBox -->
    <ControlTemplate x:Key="TextBoxControlTemplate" TargetType="TextBox">
        <Border ClipToBounds="True"
                Background="Transparent"
                BorderThickness="1"
                BorderBrush="{TemplateBinding BorderBrush}">
            <Grid Margin="3,0,0,0">
                <ScrollViewer x:Name="PART_ContentHost"
                              HorizontalAlignment="Left" VerticalAlignment="Center"/>
            </Grid>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="TextBoxWithWatermarkControlTemplate" TargetType="TextBox">
        <Border ClipToBounds="True"
                Background="Transparent"
                BorderThickness="1"
                BorderBrush="{TemplateBinding BorderBrush}">
            <Grid Margin="3,0,0,0">
                <ScrollViewer x:Name="PART_ContentHost"
                              HorizontalAlignment="Left" VerticalAlignment="Center"/>
                <TextBlock x:Name="WatermarkLabel" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Tag, Mode=OneTime}"
                           FontFamily="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=FontFamily}"
                           FontSize="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=FontSize}"
                           FontWeight="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=FontWeight}"
                           FontStyle="Italic" Foreground="#A7A9AC" Width="Auto"
                           Margin="3,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"
                           Visibility="Hidden"/>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="Text" Value="{x:Null}">
                <Setter TargetName="WatermarkLabel" Property="Visibility" Value="Visible"/>
            </Trigger>
            <Trigger Property="Text" Value="">
                <Setter TargetName="WatermarkLabel" Property="Visibility" Value="Visible"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Key="TextBoxControlStyle" TargetType="TextBox">
        <Setter Property="Template" Value="{StaticResource TextBoxControlTemplate}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Height" Value="30"/>
        <Setter Property="Width" Value="Auto"/>
        <Setter Property="BorderBrush" Value="#C9C9C9"/>
        <Setter Property="Foreground" Value="#333333"/>
        <Setter Property="FontFamily" Value="Segoe UI"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="FontWeight" Value="Normal"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="BorderBrush" Value="#4FB4F1"/>
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter Property="BorderBrush" Value="#4FB4F1"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Foreground" Value="#A7A9AC"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="TextBoxWithWatermarkControlStyle" TargetType="TextBox" BasedOn="{StaticResource TextBoxControlStyle}">
        <Setter Property="Template" Value="{StaticResource TextBoxWithWatermarkControlTemplate}"/>
    </Style>

    <!-- Button -->
    <ControlTemplate x:Key="CaptionButtonControlTemplate" TargetType="ButtonBase">
        <Border x:Name="ButtonBorder"
                ClipToBounds="True"
                Background="{TemplateBinding Background}"
                BorderThickness="{TemplateBinding BorderThickness}"
                BorderBrush="{TemplateBinding BorderBrush}">
            <Grid Background="Transparent"
                  HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                <TextBlock Text="{TemplateBinding Content}"
                           HorizontalAlignment="Center" VerticalAlignment="Center"
                           Foreground="{TemplateBinding Foreground}"
                           FontFamily="{TemplateBinding FontFamily}" FontSize="{TemplateBinding FontSize}" FontWeight="{TemplateBinding FontWeight}"/>
            </Grid>
        </Border>
    </ControlTemplate>

    <Style x:Key="BrowseButtonControlStyle" TargetType="ButtonBase">
        <Setter Property="Template" Value="{StaticResource CaptionButtonControlTemplate}"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Height" Value="30"/>
        <Setter Property="Width" Value="30"/>
        <Setter Property="Background" Value="#E0E4E8"/>
        <Setter Property="Foreground" Value="#333333"/>
        <Setter Property="FontFamily" Value="Segoe UI"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="FontWeight" Value="SemiBold"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="BorderBrush" Value="#4FB4F1"/>
        <Setter Property="Content" Value="..."/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#CDD0D4"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="#B4B7BA"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Background" Value="#EFF1F3"/>
                <Setter Property="Foreground" Value="#A7A9AC"/>
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter Property="BorderThickness" Value="1"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="GoButtonControlStyle" TargetType="ButtonBase" BasedOn="{StaticResource BrowseButtonControlStyle}">
        <Setter Property="Width" Value="100"/>
        <Setter Property="Background" Value="#4B9C09"/>
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="FontWeight" Value="Normal"/>
        <Setter Property="Content" Value="GO"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#458F08"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="#3a7807"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="AnalyzeButtonControlStyle" TargetType="ButtonBase" BasedOn="{StaticResource BrowseButtonControlStyle}">
        <Setter Property="Width" Value="100"/>
        <Setter Property="Background" Value="#4B9C09"/>
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="FontWeight" Value="Normal"/>
        <Setter Property="Content" Value="Analyze"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#458F08"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="#3a7807"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="OkButtonControlStyle" TargetType="ButtonBase" BasedOn="{StaticResource BrowseButtonControlStyle}">
        <Setter Property="Width" Value="100"/>
        <Setter Property="FontWeight" Value="Normal"/>
        <Setter Property="Content" Value="OK"/>
    </Style>
    
    <!-- ComboBox -->
    <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="#BCE3FE"/>
    <SolidColorBrush x:Key="{x:Static SystemColors.HighlightTextBrushKey}" Color="#333333"/>

    <ControlTemplate x:Key="ComboBoxToggleButtonTemplate" TargetType="ToggleButton">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition Width="27"/>
            </Grid.ColumnDefinitions>
            <Border x:Name="Border"
                    Grid.Column="0" Grid.ColumnSpan="2"
                    Background="Transparent"
                    BorderThickness="1"
                    BorderBrush="{TemplateBinding BorderBrush}"/>
            <Border x:Name="ButtonBorder"
                    Grid.Column="1" Margin="1"
                    BorderThickness="0"
                    Background="{TemplateBinding Background}"/>
            <Path x:Name="Arrow"
                  Grid.Column="1"
                  Data="M0,0 L0,2 L4,6 L8,2 L8,0 L4,4 z"
                  Fill="{TemplateBinding Foreground}"
                  HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Grid>
    </ControlTemplate>

    <Style x:Key="ComboBoxToggleButtonStyle" TargetType="ToggleButton">
        <Setter Property="Template" Value="{StaticResource ComboBoxToggleButtonTemplate}"/>
        <Setter Property="Focusable" Value="false"/>
        <Setter Property="ClickMode" Value="Press"/>
    </Style>

    <ControlTemplate x:Key="ComboBoxTextBoxTemplate" TargetType="TextBox">
        <Border Name="PART_ContentHost" Focusable="False"/>
    </ControlTemplate>

    <ControlTemplate x:Key="ComboBoxFlatTemplate" TargetType="ComboBox">
        <Grid>
            <ToggleButton Style="{StaticResource ComboBoxToggleButtonStyle}"
                          IsChecked="{Binding Path=IsDropDownOpen, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                          Background="{TemplateBinding Background}"
                          Foreground="{TemplateBinding Foreground}"
                          BorderBrush="{TemplateBinding BorderBrush}"/>
            <ContentPresenter x:Name="ContentSite"
                              Margin="7,2,23,2"
                              IsHitTestVisible="False"
                              Content="{TemplateBinding SelectionBoxItem}"
                              ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
                              ContentTemplateSelector="{TemplateBinding ItemTemplateSelector}"
                              HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <TextBox x:Name="PART_EditableTextBox"
                     Template="{StaticResource ComboBoxTextBoxTemplate}"
                     Margin="3,3,23,3"
                     Background="Transparent"
                     Focusable="True"
                     Visibility="Hidden"
                     HorizontalAlignment="Left" VerticalAlignment="Center"
                     IsReadOnly="{TemplateBinding IsReadOnly}"
                     Foreground="{TemplateBinding Foreground}"
                     FontFamily="{TemplateBinding FontFamily}"
                     FontSize="{TemplateBinding FontSize}"
                     FontWeight="{TemplateBinding FontWeight}"/>
            <Popup x:Name="Popup"
                   PopupAnimation="Slide"
                   Placement="Bottom"
                   Focusable="False"
                   AllowsTransparency="True"
                   IsOpen="{TemplateBinding IsDropDownOpen}">
                <Grid x:Name="DropDown"
                      SnapsToDevicePixels="True"
                      MinWidth="{TemplateBinding ActualWidth}"
                      MaxHeight="{TemplateBinding MaxDropDownHeight}">
                    <Border Name="DropDownBorder"
                            Margin="0,1,0,0"
                            Background="White"
                            BorderThickness="1" 
                            BorderBrush="{TemplateBinding BorderBrush}"/>
                    <ScrollViewer Margin="4" SnapsToDevicePixels="True">
                        <ItemsPresenter KeyboardNavigation.DirectionalNavigation="Contained"/>
                    </ScrollViewer>
                </Grid>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="HasItems" Value="False">
                <Setter TargetName="DropDownBorder" Property="MinHeight" Value="95"/>
            </Trigger>
            <Trigger Property="IsGrouping" Value="True">
                <Setter Property="ScrollViewer.CanContentScroll" Value="False"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Key="ComboBoxFlatStyle" TargetType="{x:Type ComboBox}">
        <Setter Property="Template" Value="{StaticResource ComboBoxFlatTemplate}"/>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto"/>
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
        <Setter Property="ScrollViewer.CanContentScroll" Value="True"/>
        <Setter Property="Height" Value="30"/>
        <Setter Property="Background" Value="#E0E4E8"/>
        <Setter Property="Foreground" Value="#333333"/>
        <Setter Property="BorderBrush" Value="#C9C9C9"/>
        <Setter Property="FontFamily" Value="Segoe UI"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="FontWeight" Value="Normal"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="OverridesDefaultStyle" Value="True"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#CDD0D4"/>
            </Trigger>
            <Trigger Property="IsDropDownOpen" Value="True">
                <Setter Property="Background" Value="#B4B7BA"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Background" Value="#EFF1F3"/>
                <Setter Property="Foreground" Value="#A7A9AC"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <!-- TextBlock -->
    <Style  x:Key="HyperLinkStyle" TargetType="{x:Type TextBlock}">
        <Setter Property="Foreground" Value="#3594ff"/>
        <Setter Property="FontFamily" Value="Segoe UI"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="FontWeight" Value="Normal"/>
        <Setter Property="TextBlock.TextDecorations" Value="{x:Null}"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="true">
                <Setter Property="TextBlock.TextDecorations" Value="Underline"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="NormalTextBloclStyle" TargetType="{x:Type TextBlock}">
        <Setter Property="Foreground" Value="#333333"/>
        <Setter Property="FontFamily" Value="Segoe UI"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="FontWeight" Value="Normal"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>

    <!-- BulletDecorator -->
    <ControlTemplate x:Key="BulletTemplate" TargetType="{x:Type ContentControl}">
        <BulletDecorator>
            <BulletDecorator.Bullet>
                <Rectangle Height="5" Width="5" Fill="#333333"/>
            </BulletDecorator.Bullet>
            <ContentPresenter Margin="5,0,0,0"/>
        </BulletDecorator>
    </ControlTemplate>

</ResourceDictionary>