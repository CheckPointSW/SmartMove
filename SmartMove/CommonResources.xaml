    <BitmapImage x:Key="CpLogo" UriSource="/Resources/CPLogo.png"/>
    <BitmapImage x:Key="ErrorIcon" UriSource="/Resources/big_error.png"/>
    <BitmapImage x:Key="WarningIcon" UriSource="/Resources/big_warning.png"/>
    <BitmapImage x:Key="InfoIcon" UriSource="/Resources/big_info.png"/> 

