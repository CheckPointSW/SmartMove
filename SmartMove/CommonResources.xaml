    <!-- Image resources -->
    <BitmapImage x:Key="AnalyzeIcon" UriSource="/Resources/AnalyzeIcon.png"/>
    <BitmapImage x:Key="AnalyzeLogo" UriSource="/Resources/AnalyzeLogo.png"/>
    <BitmapImage x:Key="ConversionIcon" UriSource="/Resources/ConversionIcon.png"/>
